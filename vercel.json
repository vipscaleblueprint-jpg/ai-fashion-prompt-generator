{
  "buildCommand": "pnpm build",
  "outputDirectory": "dist/spa",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/piapi/kling/task/:taskId",
      "destination": "/api/kling?taskId=:taskId"
    },
    {
      "source": "/api/piapi/kling/task",
      "destination": "/api/kling"
    },
    {
      "source": "/api/kling-status",
      "destination": "/api/kling"
    },
    {
      "source": "/api/kling-task",
      "destination": "/api/kling"
    },
    {
      "source": "/api/kling-task",
      "destination": "/api/kling"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/**/*": {
      "maxDuration": 60
    }
  }
}